{
    "compilerOptions": {
        "lib": ["es5"],
        "noEmitOnError": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "alwaysStrict": true,
        "pretty": true,
        "preserveConstEnums": true,
        "skipLibCheck": true,
        "stripInternal": false,
        "sourceMap": true,
        "target": "es5",
        "newLine": "lf",
        "types": [],
        "composable": true,
        "declaration": true
    }
}
