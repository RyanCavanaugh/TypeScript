interface Boolean {
    doStuff(): string;
>doStuff : () => string
>        : ^^^^^^      

    doOtherStuff<T>(x: T): T;
>doOtherStuff : <T>(x: T) => T
>             : ^ ^^ ^^ ^^^^^ 
>x : T
>  : ^
}

var x = true;
>x : boolean
>  : ^^^^^^^
>true : true
>     : ^^^^

var a: string = x.doStuff();
>a : string
